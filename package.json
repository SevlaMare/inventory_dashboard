{
  "private": true,
  "name": "app",
  "version": "0.0.0",
  "type": "module",
  "main": "main.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "notel": "yarn config set enableTelemetry 0",
    "clean": "rm -rf ./node_modules",
    "test": "vitest src/tests",
    "e2e": "yarn playwright test",
    "tests:ui": "yarn playwright test --ui",
    "report": "yarn playwright show-report",
    "css:minify": "npx tailwindcss -o build.css --minify",
    "api": "json-server --port 4000 --watch mock_db.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.1.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.6"
  },
  "optionalDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "prettier": "3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "devEngines": {
    "node": "^20.15.0",
    "yarn": ">=1 <2",
    "npm": "Use yarn instead of NPM",
    "pnpm": "Use yarn instead of PNPM"
  }
}
